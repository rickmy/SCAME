@model SCAME.Models.Paciente

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Paciente</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cedula)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cedula)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.PrimerNombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PrimerNombre) @Html.DisplayFor(model => model.SegundoNombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.PrimerApellido)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PrimerApellido) @Html.DisplayFor(model => model.SegundoApellido)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Direccion)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Direccion)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.PruebaCovid)
        </dt>
        <dd class="col-sm-10">
            <a class="icon-sli-docs" href="@("/PdfPac/" + Model.PruebaCovid)"> Prueba</a>
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CasoConfirmado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CasoConfirmado)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Discapacidad)
